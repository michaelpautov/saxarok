{
  "name": "saxarok",
  "version": "1.0.0",
  "description": "AI Tutor Telegram Bot for Salon Depilation Training",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:client": "cd client && pnpm run dev",
    "build": "tsc -p tsconfig.json && cd client && pnpm run build",
    "build:server": "tsc -p tsconfig.json",
    "build:client": "cd client && pnpm run build",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "telegram",
    "bot",
    "ai",
    "gemini",
    "tutor",
    "fastify"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/cookie": "^11.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@fastify/sensible": "^6.0.1",
    "@fastify/static": "^8.0.2",
    "@google-cloud/speech": "^7.2.0",
    "@google/genai": "^0.3.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.2.0",
    "fastify-plugin": "^5.0.1",
    "grammy": "^1.38.2",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  }
}
